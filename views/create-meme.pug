extends layout
block content
    h1= 'Create New Meme'
    form(action="/meme/create" method="post")
        input(type="hidden" name='_csrf' value=csrfToken)
        label(for="name") Meme Description:
        input(type="text" name="name" value= meme.name required)
        label(for="meme_url") Meme Link:
        input(type="text" name="meme_url" value= meme.meme_url)
        button(type='submit' class='btn') Create Meme
