extends layout

append head
  //- add page specific styles by appending to the head
  link(rel="stylesheet" href="/stylesheets/index.css")
  //- add page specific js
  script(src="/javascripts/index.js" type="module" defer)

block content
  h1= 'memes'
  div
    each meme in memes
      h2 #{meme.name}
      img(src=meme.meme_url)
